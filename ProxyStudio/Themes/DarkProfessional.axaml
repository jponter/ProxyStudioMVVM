<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <!-- My Custom Theme -->
  <!-- Description: A custom theme created with ProxyStudio Theme Editor -->
  <!-- Author: james -->
  <!-- Version: 1.0.0 -->
  <!-- Generated: 2025-08-05 16:50:46 -->

  <Styles.Resources>

    <!-- Primary Colors -->
    <Color x:Key="PrimaryColor">#3498db</Color>
    <Color x:Key="PrimaryHoverColor">#2980b9</Color>
    <Color x:Key="SecondaryColor">#95a5a6</Color>
    <Color x:Key="SecondaryHoverColor">#7f8c8d</Color>

    <!-- Background Colors -->
    <Color x:Key="BackgroundPrimaryColor">#2c3e50</Color>
    <Color x:Key="BackgroundSecondaryColor">#34495e</Color>
    <Color x:Key="BackgroundTertiaryColor">#3c4f66</Color>
    <Color x:Key="CardBackgroundColor">#34495e</Color>
    <Color x:Key="SurfaceColor">#3c4f66</Color>
    <Color x:Key="AccentBackgroundColor">#1e3a5f</Color>
    <Color x:Key="HoverBackgroundColor">#404040</Color>
    <Color x:Key="ActiveBackgroundColor">#4a4a4a</Color>
    <Color x:Key="ModalBackgroundColor">#2c3e50</Color>
    <Color x:Key="OverlayBackgroundColor">#000000</Color>
    <Color x:Key="PreviewBackgroundColor">#2c3e50</Color>
    <Color x:Key="BorderColor">#556983</Color>
    <Color x:Key="PanelBackgroundColor">#34495e</Color>
    <Color x:Key="ContentAreaColor">#2c3e50</Color>
    <Color x:Key="HeaderBackgroundColor">#34495e</Color>
    <Color x:Key="SeparatorColor">#556983</Color>
    <Color x:Key="InputBackgroundColor">#34495e</Color>
    <Color x:Key="ButtonShadowColor">#000000</Color>

    <!-- Text Colors -->
    <Color x:Key="TextPrimaryColor">#ffffff</Color>
    <Color x:Key="TextSecondaryColor">#bdc3c7</Color>
    <Color x:Key="TextTertiaryColor">#95a5a6</Color>
    <Color x:Key="TextOnPrimaryColor">#ffffff</Color>
    <Color x:Key="CaptionTextColor">#95a5a6</Color>
    <Color x:Key="DisabledTextColor">#7f8c8d</Color>
    <Color x:Key="InverseTextColor">#2c3e50</Color>
    <Color x:Key="TextMutedColor">#95a5a6</Color>
    <Color x:Key="TextLinkColor">#3498db</Color>

    <!-- Status Colors -->
    <Color x:Key="SuccessColor">#27ae60</Color>
    <Color x:Key="SuccessHoverColor">#229954</Color>
    <Color x:Key="WarningColor">#f39c12</Color>
    <Color x:Key="WarningHoverColor">#e67e22</Color>
    <Color x:Key="ErrorColor">#e74c3c</Color>
    <Color x:Key="ErrorHoverColor">#c0392b</Color>
    <Color x:Key="InfoColor">#3498db</Color>
    <Color x:Key="InfoHoverColor">#2980b9</Color>
    <Color x:Key="InfoLightColor">#1e3a5f</Color>
    <Color x:Key="SuccessLightColor">#1e4d2b</Color>
    <Color x:Key="WarningLightColor">#4d3319</Color>
    <Color x:Key="ErrorLightColor">#4d1f23</Color>

    <!-- Brushes -->
    <SolidColorBrush x:Key="PrimaryBrush" Color="{DynamicResource PrimaryColor}"/>
    <SolidColorBrush x:Key="PrimaryHoverBrush" Color="{DynamicResource PrimaryHoverColor}"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{DynamicResource SecondaryColor}"/>
    <SolidColorBrush x:Key="SecondaryHoverBrush" Color="{DynamicResource SecondaryHoverColor}"/>
    <SolidColorBrush x:Key="BackgroundPrimaryBrush" Color="{DynamicResource BackgroundPrimaryColor}"/>
    <SolidColorBrush x:Key="BackgroundSecondaryBrush" Color="{DynamicResource BackgroundSecondaryColor}"/>
    <SolidColorBrush x:Key="BackgroundTertiaryBrush" Color="{DynamicResource BackgroundTertiaryColor}"/>
    <SolidColorBrush x:Key="CardBackgroundBrush" Color="{DynamicResource CardBackgroundColor}"/>
    <SolidColorBrush x:Key="SurfaceBrush" Color="{DynamicResource SurfaceColor}"/>
    <SolidColorBrush x:Key="AccentBackgroundBrush" Color="{DynamicResource AccentBackgroundColor}"/>
    <SolidColorBrush x:Key="HoverBackgroundBrush" Color="{DynamicResource HoverBackgroundColor}"/>
    <SolidColorBrush x:Key="ActiveBackgroundBrush" Color="{DynamicResource ActiveBackgroundColor}"/>
    <SolidColorBrush x:Key="ModalBackgroundBrush" Color="{DynamicResource ModalBackgroundColor}"/>
    <SolidColorBrush x:Key="OverlayBackgroundBrush" Color="{DynamicResource OverlayBackgroundColor}"/>
    <SolidColorBrush x:Key="PreviewBackgroundBrush" Color="{DynamicResource PreviewBackgroundColor}"/>
    <SolidColorBrush x:Key="BorderBrush" Color="{DynamicResource BorderColor}"/>
    <SolidColorBrush x:Key="PanelBackgroundBrush" Color="{DynamicResource PanelBackgroundColor}"/>
    <SolidColorBrush x:Key="ContentAreaBrush" Color="{DynamicResource ContentAreaColor}"/>
    <SolidColorBrush x:Key="HeaderBackgroundBrush" Color="{DynamicResource HeaderBackgroundColor}"/>
    <SolidColorBrush x:Key="SeparatorBrush" Color="{DynamicResource SeparatorColor}"/>
    <SolidColorBrush x:Key="InputBackgroundBrush" Color="{DynamicResource InputBackgroundColor}"/>
    <SolidColorBrush x:Key="ButtonShadowBrush" Color="{DynamicResource ButtonShadowColor}"/>
    <SolidColorBrush x:Key="TextPrimaryBrush" Color="{DynamicResource TextPrimaryColor}"/>
    <SolidColorBrush x:Key="TextSecondaryBrush" Color="{DynamicResource TextSecondaryColor}"/>
    <SolidColorBrush x:Key="TextTertiaryBrush" Color="{DynamicResource TextTertiaryColor}"/>
    <SolidColorBrush x:Key="TextOnPrimaryBrush" Color="{DynamicResource TextOnPrimaryColor}"/>
    <SolidColorBrush x:Key="CaptionTextBrush" Color="{DynamicResource CaptionTextColor}"/>
    <SolidColorBrush x:Key="DisabledTextBrush" Color="{DynamicResource DisabledTextColor}"/>
    <SolidColorBrush x:Key="InverseTextBrush" Color="{DynamicResource InverseTextColor}"/>
    <SolidColorBrush x:Key="TextMutedBrush" Color="{DynamicResource TextMutedColor}"/>
    <SolidColorBrush x:Key="TextLinkBrush" Color="{DynamicResource TextLinkColor}"/>
    <SolidColorBrush x:Key="SuccessBrush" Color="{DynamicResource SuccessColor}"/>
    <SolidColorBrush x:Key="SuccessHoverBrush" Color="{DynamicResource SuccessHoverColor}"/>
    <SolidColorBrush x:Key="WarningBrush" Color="{DynamicResource WarningColor}"/>
    <SolidColorBrush x:Key="WarningHoverBrush" Color="{DynamicResource WarningHoverColor}"/>
    <SolidColorBrush x:Key="ErrorBrush" Color="{DynamicResource ErrorColor}"/>
    <SolidColorBrush x:Key="ErrorHoverBrush" Color="{DynamicResource ErrorHoverColor}"/>
    <SolidColorBrush x:Key="InfoBrush" Color="{DynamicResource InfoColor}"/>
    <SolidColorBrush x:Key="InfoHoverBrush" Color="{DynamicResource InfoHoverColor}"/>
    <SolidColorBrush x:Key="InfoLightBrush" Color="{DynamicResource InfoLightColor}"/>
    <SolidColorBrush x:Key="SuccessLightBrush" Color="{DynamicResource SuccessLightColor}"/>
    <SolidColorBrush x:Key="WarningLightBrush" Color="{DynamicResource WarningLightColor}"/>
    <SolidColorBrush x:Key="ErrorLightBrush" Color="{DynamicResource ErrorLightColor}"/>

  </Styles.Resources>

  <!-- Control Styles -->

  <Style Selector="Window">
    <Setter Property="Background" Value="{DynamicResource BackgroundPrimaryBrush}"/>
    <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
  </Style>

  <Style Selector="Button.primary">
    <Setter Property="Background" Value="{DynamicResource PrimaryBrush}"/>
    <Setter Property="Foreground" Value="{DynamicResource TextOnPrimaryBrush}"/>
    <Setter Property="BorderBrush" Value="{DynamicResource PrimaryBrush}"/>
    <Setter Property="BorderThickness" Value="1"/>
    <Setter Property="CornerRadius" Value="6"/>
    <Setter Property="Padding" Value="16,8"/>
    <Setter Property="FontWeight" Value="SemiBold"/>
  </Style>

  <Style Selector="Button.primary:pointerover">
    <Setter Property="Background" Value="{DynamicResource PrimaryHoverBrush}"/>
    <Setter Property="BorderBrush" Value="{DynamicResource PrimaryHoverBrush}"/>
  </Style>

  <Style Selector="Button.secondary">
    <Setter Property="Background" Value="{DynamicResource SecondaryBrush}"/>
    <Setter Property="Foreground" Value="{DynamicResource TextOnPrimaryBrush}"/>
    <Setter Property="BorderBrush" Value="{DynamicResource SecondaryBrush}"/>
    <Setter Property="BorderThickness" Value="1"/>
    <Setter Property="CornerRadius" Value="6"/>
    <Setter Property="Padding" Value="16,8"/>
  </Style>

  <Style Selector="Button.secondary:pointerover">
    <Setter Property="Background" Value="{DynamicResource SecondaryHoverBrush}"/>
    <Setter Property="BorderBrush" Value="{DynamicResource SecondaryHoverBrush}"/>
  </Style>

  <Style Selector="Button.success">
    <Setter Property="Background" Value="{DynamicResource SuccessBrush}"/>
    <Setter Property="Foreground" Value="{DynamicResource TextOnPrimaryBrush}"/>
    <Setter Property="BorderBrush" Value="{DynamicResource SuccessBrush}"/>
    <Setter Property="BorderThickness" Value="1"/>
    <Setter Property="CornerRadius" Value="6"/>
    <Setter Property="Padding" Value="16,8"/>
  </Style>

  <Style Selector="Button.success:pointerover">
    <Setter Property="Background" Value="{DynamicResource SuccessHoverBrush}"/>
    <Setter Property="BorderBrush" Value="{DynamicResource SuccessHoverBrush}"/>
  </Style>

  <Style Selector="Button.icon">
    <Setter Property="Background" Value="{DynamicResource BackgroundSecondaryBrush}"/>
    <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
    <Setter Property="BorderBrush" Value="{DynamicResource BackgroundTertiaryBrush}"/>
    <Setter Property="BorderThickness" Value="1"/>
    <Setter Property="CornerRadius" Value="50"/>
    <Setter Property="Width" Value="40"/>
    <Setter Property="Height" Value="40"/>
    <Setter Property="Padding" Value="0"/>
  </Style>

  <Style Selector="Border.card">
    <Setter Property="Background" Value="{DynamicResource CardBackgroundBrush}"/>
    <Setter Property="BorderBrush" Value="{DynamicResource BackgroundSecondaryBrush}"/>
    <Setter Property="BorderThickness" Value="1"/>
    <Setter Property="CornerRadius" Value="8"/>
    <Setter Property="Padding" Value="16"/>
  </Style>

  <Style Selector="Border.card-compact">
    <Setter Property="Background" Value="{DynamicResource CardBackgroundBrush}"/>
    <Setter Property="BorderBrush" Value="{DynamicResource BackgroundSecondaryBrush}"/>
    <Setter Property="BorderThickness" Value="1"/>
    <Setter Property="CornerRadius" Value="6"/>
    <Setter Property="Padding" Value="12"/>
  </Style>

  <Style Selector="Border.card-elevated">
    <Setter Property="Background" Value="{DynamicResource CardBackgroundBrush}"/>
    <Setter Property="BorderBrush" Value="{DynamicResource BackgroundSecondaryBrush}"/>
    <Setter Property="BorderThickness" Value="1"/>
    <Setter Property="CornerRadius" Value="8"/>
    <Setter Property="Padding" Value="16"/>
  </Style>

  <Style Selector="TextBox.modern">
    <Setter Property="Background" Value="{DynamicResource SurfaceBrush}"/>
    <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
    <Setter Property="BorderBrush" Value="{DynamicResource BackgroundSecondaryBrush}"/>
    <Setter Property="BorderThickness" Value="1"/>
    <Setter Property="CornerRadius" Value="4"/>
    <Setter Property="Padding" Value="8"/>
  </Style>

  <Style Selector="TextBox.modern:focus">
    <Setter Property="BorderBrush" Value="{DynamicResource PrimaryBrush}"/>
    <Setter Property="BorderThickness" Value="2"/>
  </Style>

  <Style Selector="ComboBox.modern">
    <Setter Property="Background" Value="{DynamicResource SurfaceBrush}"/>
    <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
    <Setter Property="BorderBrush" Value="{DynamicResource BackgroundSecondaryBrush}"/>
    <Setter Property="BorderThickness" Value="1"/>
    <Setter Property="CornerRadius" Value="4"/>
    <Setter Property="Padding" Value="8"/>
  </Style>

  <!-- Typography Classes -->
  <Style Selector="TextBlock.heading-large">
    <Setter Property="FontSize" Value="32"/>
    <Setter Property="FontWeight" Value="Bold"/>
    <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
  </Style>

  <Style Selector="TextBlock.heading-medium">
    <Setter Property="FontSize" Value="24"/>
    <Setter Property="FontWeight" Value="SemiBold"/>
    <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
  </Style>

  <Style Selector="TextBlock.heading-small">
    <Setter Property="FontSize" Value="18"/>
    <Setter Property="FontWeight" Value="SemiBold"/>
    <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
  </Style>

  <Style Selector="TextBlock.body-large">
    <Setter Property="FontSize" Value="16"/>
    <Setter Property="FontWeight" Value="Normal"/>
    <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
  </Style>

  <Style Selector="TextBlock.body-medium">
    <Setter Property="FontSize" Value="14"/>
    <Setter Property="FontWeight" Value="Normal"/>
    <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
  </Style>

  <Style Selector="TextBlock.body-small">
    <Setter Property="FontSize" Value="12"/>
    <Setter Property="FontWeight" Value="Normal"/>
    <Setter Property="Foreground" Value="{DynamicResource TextSecondaryBrush}"/>
  </Style>

  <Style Selector="TextBlock.caption">
    <Setter Property="FontSize" Value="11"/>
    <Setter Property="FontWeight" Value="SemiBold"/>
    <Setter Property="Foreground" Value="{DynamicResource TextTertiaryBrush}"/>
  </Style>

  <Style Selector="Border.highlight-primary">
    <Setter Property="BorderBrush" Value="{DynamicResource PrimaryBrush}"/>
    <Setter Property="BorderThickness" Value="2"/>
  </Style>

  <Style Selector="Border.highlight-success">
    <Setter Property="BorderBrush" Value="{DynamicResource SuccessBrush}"/>
    <Setter Property="BorderThickness" Value="2"/>
  </Style>

  <Style Selector="Border.badge-success">
    <Setter Property="Background" Value="{DynamicResource SuccessBrush}"/>
    <Setter Property="CornerRadius" Value="12"/>
    <Setter Property="Padding" Value="8,4"/>
  </Style>

  <Style Selector="Border.badge-error">
    <Setter Property="Background" Value="{DynamicResource ErrorBrush}"/>
    <Setter Property="CornerRadius" Value="12"/>
    <Setter Property="Padding" Value="8,4"/>
  </Style>

  <Style Selector="Border.badge-warning">
    <Setter Property="Background" Value="{DynamicResource WarningBrush}"/>
    <Setter Property="CornerRadius" Value="12"/>
    <Setter Property="Padding" Value="8,4"/>
  </Style>

  <Style Selector="Border.badge-info">
    <Setter Property="Background" Value="{DynamicResource InfoBrush}"/>
    <Setter Property="CornerRadius" Value="12"/>
    <Setter Property="Padding" Value="8,4"/>
  </Style>
</Styles>
